# Wait for FBL to start
echo "Waiting for FBL to open ports ${ffbo_port} and ${port}..."
if wait_until_port_used "${host}:${ffbo_port}" 120; then
  echo "Discovered FFBO listening on port ${ffbo_port}!"
else
  echo "Timed out waiting for FFBO to open port ${ffbo_port}!"
  clean_up 1
fi
if wait_until_port_used "${host}:${port}" 120; then
  echo "Discovered FBL listening on port ${port}!"
else
  echo "Timed out waiting for FBL to open port ${port}!"
  clean_up 1
fi
